%YAML 1.1
---
version: 4

compiler-assemblies:
  - $(BuildContentDir)/Playroom.Compilers.dll

compiler-settings:
  - name: CopyCompiler
    extensions:
    - inputs:  .png
      outputs: .png
    - inputs:  .wav
      outputs: .wav

properties:
  OutputDir: $(ContentFileDir)../Content

targets:
  - name:     ExampleSvgPng
    inputs:   [$(InputDir)/Example.svg]
    outputs:  [$(OutputDir)/$(TargetName).png]
    parameters:
      Width:  100
      Height: 100

